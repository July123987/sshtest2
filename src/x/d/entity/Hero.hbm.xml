<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="x.d.entity">
	 <class name="Hero" table="dotahero">
	       <id name="id" column="id">
	       		<generator class="native"/>
	       </id>
	      <property name="TrideName" column="TrideName"/>
	       <property name="Price" column="Price"/>
	       <property name="TransportWay" column="TransportWay"/>
	       <property name="Payment" column="Payment"/>
	       <property name="CustomerId" column="CustomerId"/>
	       <property name="CustomerName" column="CustomerName"/>
    </class>
</hibernate-mapping>